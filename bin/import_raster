#!/bin/bash

# CD into Data Directory as $HOME
export HOME="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && cd .. && pwd )"
cd $HOME

# Set Up Environment
source grass_enviroment.sh

# Enable Log
exec > >(tee -i $LOGS_DIR/$(date +"%Y_%m_%d_%H_%M_%S")'.log')
exec 2>&1
date

# Import raster
file=$(basename "$1")
r.import --overwrite input="$1" output="${file%.*}"

date
echo " ========== DONE =========="

grass_cleanup
