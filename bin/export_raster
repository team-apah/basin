#!/bin/bash

# CD into Data Directory as $HOME
export HOME="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && cd .. && pwd )"
cd $HOME

# Set Up Environment
source grass_enviroment.sh

# Import raster
r.out.gdal\
    --overwrite\
    input=_$1\
    output=$2

date
echo " ========== DONE =========="

grass_cleanup
